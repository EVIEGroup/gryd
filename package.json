{
  "name": "decentranet",
  "version": "0.0.1",
  "description": "Decentralised Node Network",
  "main": "./dist/index.js",
  "exports": "./dist/index.js",
  "scripts": {
    "test": "jest --no-cache",
    "build": "tsc",
    "start": "node ./dist/index.js",
    "dev": "tsc-watch --onSuccess \"npm run start -- --port 8080\"",
    "dev2": "tsc-watch --onSuccess \"npm run start -- --port 8082 --masterport 8081\"",
    "dev3": "tsc-watch --onSuccess \"npm run start -- --port 8084 --masterport 8083\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/EVIEGroup/decentranet.git"
  },
  "keywords": [
    "decentralised",
    "decentralized",
    "network"
  ],
  "author": "Upperfoot",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/EVIEGroup/decentranet/issues"
  },
  "homepage": "https://github.com/EVIEGroup/decentranet#readme",
  "devDependencies": {
    "@types/jest": "^27.0.3",
    "@types/node": "^16.11.10",
    "@types/uuid": "^8.3.3",
    "@types/ws": "^8.2.0",
    "jest": "^27.3.1",
    "ts-jest": "^27.0.7",
    "tsc-watch": "^4.5.0"
  },
  "dependencies": {
    "assemblyscript": "^0.19.20",
    "typescript": "^4.5.2",
    "uuid": "^8.3.2",
    "vm2": "^3.9.5",
    "ws": "^8.3.0"
  }
